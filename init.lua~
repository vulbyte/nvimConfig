io.write('LOADING: ./nvim/init.lua')

-- yepp
require('vulbyte.init')
use {
    'rrethy/vim-hexokinase',
    run = function()
        local install_status = vim.fn.system('make hexokinase')
        if vim.v.shell_error == 0 then
            print('vim-hexokinase installed successfully.')
        else
            print('Error installing vim-hexokinase:', install_status)
        end
    end
}

--io.write("\27[31m\n+++++++++++++++++++++++++++++LOADED: ./nvim/init.lua\n\27[0m")
print("\27[31m\n==============================LOADED: ./nvim/init.lua")
